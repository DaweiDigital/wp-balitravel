<!doctype html>
<html class="no-js no-touch {{ get_browser_data() }}" lang="cs">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="apple-mobile-web-app-capable" content="yes" />

    <link rel="apple-touch-icon" sizes="180x180" href="{{ theme.link }}/static/images/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ theme.link }}/static/images/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ theme.link }}/static/images/favicon/favicon-16x16.png">
    <link rel="manifest" href="{{ theme.link }}/static/images/favicon/site.webmanifest">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">
    {{ fn( 'wp_head' ) }}
</head>

<body {{ fn( 'body_class' ) }}>
    <!-- .header -->
    <header class="header js-header-navbar">
        <div class="upper-header">
            <div class="container">
                <div class="row">
                    <div class="col-md-6">
                        <ul class="m-0 d-flex justify-content-start align-items-center">
                            {% if global.address is not empty %}
                            <li>
                                <a href="{{global.address.url}}" target="{{global.address.target}}"
                                    class="upper-header-link">
                                    <i
                                        class="fal fa-map-marker-alt fs-17 color-primary mr-2"></i>{{global.address.title}}
                                </a>
                            </li>
                            {% endif %}
                            {% if global.telNumber is not empty %}
                            <li>
                                <a href="https://wa.me/{{global.telNumber}}?text=I'm%20interested%20in%20your%20car%20for%20sale"
                                    target="_blank" class="upper-header-link">
                                    <i class="fab fa-whatsapp fs-17 color-whatsapp mr-2"></i>{{global.telNumber}}
                                </a>
                            </li>
                            {% endif %}
                        </ul>
                    </div>
                    <div class="col-md-6 d-flex align-items-center justify-content-end">
                        <span class="upper-header-link">Follow us:</span>
                        <ul class="social-list justify-content-end theme-v1">
                            {% if global.facebook is not empty %}
                            <li class="social-list-item">
                                <a href="{{global.facebook.url}}" target="{{global.facebook.target}}"
                                    class="social-list-link">
                                    <i class="fab fa-facebook-f"></i>
                                </a>
                            </li>
                            {% endif %}
                            {% if global.instagram is not empty %}
                            <li class="social-list-item">
                                <a href="{{global.instagram.url}}" target="{{global.instagram.target}}"
                                    class="social-list-link">
                                    <i class="fab fa-instagram"></i>
                                </a>
                            </li>
                            {% endif %}
                            {% if global.youtube is not empty %}
                            <li class="social-list-item">
                                <a href="{{global.youtube.url}}" target="{{global.youtube.target}}"
                                    class="social-list-link">
                                    <i class="fab fa-youtube"></i>
                                </a>
                            </li>
                            {% endif %}
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="down-header py-2">
            <div class="container">
                <div class="d-flex align-items-center justify-content-between">
                    <a href="{{ fn( 'get_home_url' ) }}" class="header-brand d-flex">
                        <img src="{{ theme.link }}/static/images/gadis-bali-tour-logo.png" width="200"
                            alt="Gadis Bali Tour logo">
                    </a>
                    <div class="navbar-responsive-menu d-md-flex flex-wrap align-items-center justify-content-end">
                        <a href="{{ fn( 'get_home_url' ) }}" class="header-brand d-flex d-md-none">
                            <img src="{{ theme.link }}/static/images/gadis-bali-tour-logo.png" width="200"
                                alt="Gadis Bali Tour logo">
                        </a>
                        <span class="close-navi js-close"></span>
                        <nav class="primary-nav mr-5">
                            <ul class="p-n-list" itemscope="itemscope"
                                itemtype="http://schema.org/SiteNavigationElement">
                                {% for item in primary_nav.items %}
                                <li
                                    class="p-n-item {{ item.current or item.current_item_parent or item.current_item_ancestor ? 'is-active' : '' }}">
                                    <a href="{{ item.link }}"
                                        class="p-n-link align-items-center {{ item.class }} {% if item.children %}js-dropdown-link{% endif %} {{ item.active ? 'is-active' : '' }}">
                                        {{ item.title }}
                                        {% if item.children %}
                                        <span class="p-n-icon-wrap d-md-none">
                                            <i class="ix-arrow-right-swap"></i>
                                        </span>
                                        <i class="icon-arrow-down d-none d-md-inline-flex"></i>
                                        {% endif %}
                                    </a>
                                    {% if item.children %}
                                    <ul class="p-n-dropdown-list js-menu-dropdown">
                                        {% for item in item.children %}
                                        <li class="p-n-d-l-item {{ item.children ? 'p-n-d-l-item-has-children' : '' }}">
                                            <a href="{{ item.link}}" class="p-n-d-l-item-title">
                                                {{ item.title }}
                                            </a>
                                            {% if item.children %}
                                            <ul class="p-n-child-list">
                                                {% for item in item.children %}
                                                <li
                                                    class="p-n-ch-l-item {{ item.children ? 'p-n-ch-l-item-has-children' : '' }}">
                                                    <a href="{{ item.link }}" class="p-n-ch-l-item-link">
                                                        {{ item.title }}
                                                    </a>
                                                    {% if item.children %}
                                                    <ul class="p-n-ch-l-two-list d-flex flex-wrap align-items-center">
                                                        {% for item in item.children %}
                                                        <li
                                                            class="p-n-ch-t-l-item {{ item.children ? 'p-n-ch-t-l-item-has-children' : '' }}">
                                                            <a href="{{ item.link }}" class="p-n-ch-t-l-item-link">
                                                                {{ item.title }}
                                                            </a>
                                                        </li>
                                                        {% endfor %}
                                                    </ul>
                                                    {% endif %}
                                                </li>
                                                {% endfor %}
                                            </ul>
                                            {% endif %}
                                        </li>
                                        {% endfor %}
                                    </ul>
                                    {% endif %}
                                </li>
                                {% endfor %}
                            </ul>
                        </nav>
                        <a href="#" data-modal="1"
                            class="btn btn-primary cta-btn d-flex align-items-center justify-content-center mt-3 mt-md-0">
                            <i class="fal fa-envelope cta-btn-icon mr-3"></i>
                            {{ __( 'Contact us', 'ixtent' ) }}
                        </a>
                    </div>
                    <button type="button" class="navbar-toggle js-navbar-toggle">
                        <span class="sr-only">Menu</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
            </div>
        </div>
    </header>
    <!-- /.header -->
    <main class="main" role="main">
        {% block content %}{% endblock %}
    </main>
    <!-- .footer -->
    <footer class="footer">
        <div class="footer-cta">
            <div class="container">
                <ul class="cta-list d-flex justify-content-center align-items-center flex-wrap">
                    {% for item in global.general_cta_list %}
                    <li class="cta-list-item">
                        {% if item.ctaNumber is not empty %}
                        <div class="cta-list-number js-number-count">{{item.ctaNumber}}</div>
                        <div class="cta-list-title">{{item.ctatext}}</div>
                        {% endif %}
                        {% if item.ctaImage is not empty %}
                        <a href="{{item.ctaLink.url}}" target="{{item.ctaLink.target}}">
                            <img class="js-lazy" src="{{ theme.link }}/static/images/lazyload/lazy.png"
                                data-src="{{ item.ctaImage.sizes['medium-thumb'] }}" alt="{{ item.ctaImage.alt }}">
                        </a>
                        {% endif %}
                    </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
        <div class="website-footer bg-black">
            <div class="container py-5">
                <div class="website-upper-footer pt-5">
                    <div class="row">
                        {% for item in global.footerTabPackages %}
                            <div class="col-12 col-lg-3 mb-4">
                                <div class="text-center pos-relative js-lazy" data-bg="url({{ item.footerTabPackagesImage.sizes['medium-thumb'] }})">
                                    <div class="">
                                        <div class="">
                                            <span class="">
                                                {{item.footerTabPackagesDiscount}}%
                                            </span>
                                            Off
                                        </div>
                                        <span class="d-block">
                                            {{item.footerTabPackagesName}}
                                        </span>
                                        <span class="d-block">
                                            {{item.footerTabPackagesValid}}
                                        </span>
                                        <span class="btn btn-primary">
                                            Book Now
                                        </span>
                                    </div>
                                    <a href="{{item.footerTabPackagesLink.url}}" target="{{item.footerTabPackagesLink.target}}" class="nested-link"></a>
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                </div>
                <div class="website-bottom-footer pb-5">
                    <div class="row">
                        <div class="col-12 col-lg-3">
                            {% if global.footerFirstBoxTitle is not empty %}
                            <span class="d-block fs-18 text-uppercase color-grey-v10 font-bold font-primary mb-3">
                                {{global.footerFirstBoxTitle}}
                            </span>
                            {% endif %}
                            {% if global.footerFirstBoxText is not empty %}
                            <p class="fs-14 lh-24 color-grey-v9 mb-0">
                                {{global.footerFirstBoxText}}
                            </p>
                            {% endif %}
                        </div>
                        <div class="col-12 col-lg-3">
                            {% if global.footerSecondBoxTitle is not empty %}
                            <span class="d-block fs-18 text-uppercase color-grey-v10 font-bold font-primary mb-3">
                                {{global.footerSecondBoxTitle}}
                            </span>
                            {% endif %}
                            {% if global.footerSecondBoxText is not empty %}
                            <p class="fs-14 lh-24 color-grey-v9 mb-0">
                                {{global.footerSecondBoxText}}
                            </p>
                            {% endif %}
                        </div>
                        <div class="col-12 col-lg-3">
                            {% if global.footerThirdBoxTitle is not empty %}
                            <span class="d-block fs-18 text-uppercase color-grey-v10 font-bold font-primary mb-3">
                                {{global.footerThirdBoxTitle}}
                            </span>
                            {% endif %}
                            {% if global.footerTabMenu is not empty %}
                            <ul class="d-flex flex-wrap justify-content-between">
                                {% for item in global.footerTabMenu %}
                                <li class="col-md-6 px-0">
                                    <a href="{{item.footerTabMenuLink.url}}" target="{{item.footerTabMenuLink.target}}"
                                        class="fs-14 lh-24 color-grey-v9 hover-color-white">
                                        {{item.footerTabMenuLink.title}}
                                    </a>
                                </li>
                                {% endfor %}
                            </ul>
                            {% endif %}
                        </div>
                        <div class="col-12 col-lg-3">
                            {% if global.footerFourthBoxTitle is not empty %}
                            <span class="d-block fs-18 text-uppercase color-grey-v10 font-bold font-primary mb-3">
                                {{global.footerFourthBoxTitle}}
                            </span>
                            {% endif %}
                            {% if global.footerFourthBoxText is not empty %}
                            <p class="fs-14 lh-24 color-grey-v9 mb-0">
                                {{global.footerFourthBoxText}}
                            </p>
                            {% endif %}
                            <ul class="social-list justify-content-start mt-3 theme-sl-v2">
                                {% if global.facebook is not empty %}
                                <li class="social-list-item mr-3">
                                    <a href="{{global.facebook.url}}" target="{{global.facebook.target}}"
                                        class="social-list-link">
                                        <i class="fab fa-facebook-f"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if global.instagram is not empty %}
                                <li class="social-list-item mr-3">
                                    <a href="{{global.instagram.url}}" target="{{global.instagram.target}}"
                                        class="social-list-link">
                                        <i class="fab fa-instagram"></i>
                                    </a>
                                </li>
                                {% endif %}
                                {% if global.youtube is not empty %}
                                <li class="social-list-item">
                                    <a href="{{global.youtube.url}}" target="{{global.youtube.target}}"
                                        class="social-list-link">
                                        <i class="fab fa-youtube"></i>
                                    </a>
                                </li>
                                {% endif %}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="website-footer-address bg-white py-4">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-12 col-md-6 text-center text-lg-left mt-4 mt-lg-0">
                            <p class="m-0 fs-13">
                                {{ __( 'All rights reserved' ) }} &copy; {{ now|date('Y') }} {{ site.name }}
                            </p>
                        </div>
                        <div class="col-12 col-md-6">
                            <p class="m-0 fs-13 text-center text-lg-right">
                                <a href="http://www.daweidigital.eu" target="_blank"
                                    class="text-darkgrey text-decoration-none">{{ __( 'Website Development Dawei Digital' ) }}</a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- /.footer -->
    {% if global.modalWindowContent is not empty %}
    <div class="modal-window modal-1 js-modal-window">
        <div class="modal-content">
            <span class="close-modal js-close-modal"></span>
            <span class="h2 d-block text-center mb-2">{{ global.modalWindowTitle }}</span>
            <p class="text-center mt-0">{{ global.modalWindowText }}</p>
            <div class="mt-5">
                {{ global.modalWindowContent }}
            </div>
        </div>
        <div class="overlay js-overlay"></div>
    </div>
    {% endif %}
    <div class="overlay js-overlay-global"></div>
    {{ fn( 'wp_footer' ) }}
    {% block scripts %}{% endblock %}
</body>

</html>